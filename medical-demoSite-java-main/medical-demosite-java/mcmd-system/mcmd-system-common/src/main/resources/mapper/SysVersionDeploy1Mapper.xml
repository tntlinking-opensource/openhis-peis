<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.system.dao.SysVersionDeploy1Mapper">

    <resultMap type="com.center.medical.system.bean.model.SysVersionDeploy1" id="SysVersionDeployMap">
        <id property="id" column="id"/>
        <result property="versionId" column="version_id"/>
        <result property="ksIpId" column="ks_ip_id"/>
        <result property="address" column="address"/>
        <result property="hostName" column="host_name"/>
        <result property="branchId" column="branch_id"/>
        <result property="sqlStatus" column="sql_status"/>
        <result property="sqlRemark" column="sql_remark"/>
        <result property="sqlUpdateTime" column="sql_update_time"/>
        <result property="isManual" column="is_manual"/>
        <result property="executeTime" column="execute_time"/>
        <result property="status" column="status"/>
        <result property="remark" column="remark"/>
        <result property="message" column="message"/>
        <result property="executer" column="executer"/>
        <result property="manualExecuteTime" column="manual_execute_time"/>
        <result property="createdate" column="createdate"/>
        <result property="modifydate" column="modifydate"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectSysVersionDeployVo">
        select id,
               version_id,
               ks_ip_id,
               address,
               host_name,
               branch_id,
               sql_status,
               sql_remark,
               sql_update_time,
               is_manual,
               execute_time,
               status,
               remark,
               message,
               executer,
               manual_execute_time,
               createdate,
               modifydate
        from sys_version_deploy
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="SysVersionDeployMap">
        <include refid="selectSysVersionDeployVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="SysVersionDeployMap">
        <include refid="selectSysVersionDeployVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

