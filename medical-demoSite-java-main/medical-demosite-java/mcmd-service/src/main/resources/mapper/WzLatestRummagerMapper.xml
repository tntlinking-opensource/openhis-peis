<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.dao.WzLatestRummagerMapper">

    <resultMap type="com.center.medical.bean.model.WzLatestRummager" id="WzLatestRummagerMap">
        <id property="id" column="id"/>
        <result property="cid" column="cid"/>
        <result property="ksId" column="ks_id"/>
        <result property="createdate" column="createdate"/>
        <result property="modifydate" column="modifydate"/>
        <result property="rummagerId" column="rummager_id"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectWzLatestRummagerVo">
        select id, cid, ks_id, createdate, modifydate, rummager_id
        from md_wz_latest_rummager
    </sql>

    <!-- 查询列表数据 -->
    <select id="getList" resultMap="WzLatestRummagerMap">
        <include refid="selectWzLatestRummagerVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="WzLatestRummagerMap">
        <include refid="selectWzLatestRummagerVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

