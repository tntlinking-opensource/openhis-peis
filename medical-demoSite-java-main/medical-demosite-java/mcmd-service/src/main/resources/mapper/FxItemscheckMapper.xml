<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.dao.FxItemscheckMapper">

    <resultMap type="com.center.medical.bean.model.FxItemscheck" id="FxItemscheckMap">
        <id property="id" column="id"/>
        <result property="createdate" column="createdate"/>
        <result property="modifydate" column="modifydate"/>
        <result property="sampleId" column="sample_id"/>
        <result property="depName" column="dep_name"/>
        <result property="depId" column="dep_id"/>
        <result property="itemName" column="item_name"/>
        <result property="itemId" column="item_id"/>
        <result property="checkMale" column="check_male"/>
        <result property="checkFemale" column="check_female"/>
        <result property="checkTotal" column="check_total"/>
        <result property="allMale" column="all_male"/>
        <result property="allFemale" column="all_female"/>
        <result property="allTotal" column="all_total"/>
        <result property="perMale" column="per_male"/>
        <result property="perFemale" column="per_female"/>
        <result property="perTotal" column="per_total"/>
        <result property="reportSort" column="report_sort"/>
        <result property="rowno" column="rowno"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectFxItemscheckVo">
        select id,
               createdate,
               modifydate,
               sample_id,
               dep_name,
               dep_id,
               item_name,
               item_id,
               check_male,
               check_female,
               check_total,
               all_male,
               all_female,
               all_total,
               per_male,
               per_female,
               per_total,
               report_sort,
               rowno
        from md_fx_itemscheck
    </sql>

    <!-- 查询列表数据 -->
    <select id="getList" resultMap="FxItemscheckMap">
        <include refid="selectFxItemscheckVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="FxItemscheckMap">
        <include refid="selectFxItemscheckVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

