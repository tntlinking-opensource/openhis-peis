    <!-- 数据表字段属性 -->
    <sql id="selectClientcommonVo">
        select ID,
               KHDWMC,
               KHDWSRM,
               KHDWLXR,
               KHDH,
               SSHY,
               XSJL,
               DWJGDM,
               KHDWDZ,
               FZXID,
               XSJLID,
               LQCSTJ,
               IS_DELETE,
               CREATEDATE,
               MODIFYDATE,
               FPZT,
               FRDWMC,
               FDDBR,
               YZBM,
               QYGM,
               QYJJLX,
               ZYWSFZR,
               KHDWZCDZ,
               ZYWSGLJG,
               ZCLX,
               LSGX,
               SJZGDW,
               SJCYRS,
               LDRS,
               SCGRS,
               ZYBWHYSRS,
               ZYBWHZYCSS,
               ZYBWHYSLB,
               ZYBWHYS,
               GYLC,
               ZYYFL,
               TJTTLX,
               ZYCP,
               KHSCTJDWDZ,
               BZ
        from CLIENTCOMMON
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="ClientcommonMap">
        <include refid="selectClientcommonVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="ClientcommonMap">
        <include refid="selectClientcommonVo"/>
        <where>
            id = #{id}
        </where>
    </select>

