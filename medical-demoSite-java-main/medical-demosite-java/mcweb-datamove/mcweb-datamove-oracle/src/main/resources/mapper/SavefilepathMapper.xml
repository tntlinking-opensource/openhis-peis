<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.oracle.dao.SavefilepathMapper">

    <resultMap type="com.center.medical.datamove.oracle.bean.model.Savefilepath" id="SavefilepathMap">
        <id property="id" column="ID"/>
        <result property="ggid" column="GGID"/>
        <result property="filepath" column="FILEPATH"/>
        <result property="createdate" column="CREATEDATE"/>
        <result property="modifydate" column="MODIFYDATE"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectSavefilepathVo">
        select ID, GGID, FILEPATH, CREATEDATE, MODIFYDATE
        from SAVEFILEPATH
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="SavefilepathMap">
        <include refid="selectSavefilepathVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="SavefilepathMap">
        <include refid="selectSavefilepathVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

