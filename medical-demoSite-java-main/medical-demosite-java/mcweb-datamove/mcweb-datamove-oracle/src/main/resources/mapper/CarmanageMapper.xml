<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.oracle.dao.CarmanageMapper">

    <resultMap type="com.center.medical.datamove.oracle.bean.model.Carmanage" id="CarmanageMap">
        <id property="id" column="ID"/>
        <result property="carNo" column="CAR_NO"/>
        <result property="idDriver" column="ID_DRIVER"/>
        <result property="idOrder" column="ID_ORDER"/>
        <result property="idOrg" column="ID_ORG"/>
        <result property="orgName" column="ORG_NAME"/>
        <result property="address" column="ADDRESS"/>
        <result property="outTimeStart" column="OUT_TIME_START"/>
        <result property="outTimeEnd" column="OUT_TIME_END"/>
        <result property="isDelete" column="IS_DELETE"/>
        <result property="createdate" column="CREATEDATE"/>
        <result property="modifydate" column="MODIFYDATE"/>
        <result property="createer" column="CREATEER"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectCarmanageVo">
        select ID,
               CAR_NO,
               ID_DRIVER,
               ID_ORDER,
               ID_ORG,
               ORG_NAME,
               ADDRESS,
               OUT_TIME_START,
               OUT_TIME_END,
               IS_DELETE,
               CREATEDATE,
               MODIFYDATE,
               CREATEER
        from CARMANAGE
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="CarmanageMap">
        <include refid="selectCarmanageVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="CarmanageMap">
        <include refid="selectCarmanageVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

