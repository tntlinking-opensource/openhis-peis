<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.oracle.dao.AppLogMapper">

    <resultMap type="com.center.medical.datamove.oracle.bean.model.AppLog" id="AppLogMap">
        <id property="id" column="ID"/>
        <result property="modifydate" column="MODIFYDATE"/>
        <result property="createdate" column="CREATEDATE"/>
        <result property="operateType" column="OPERATE_TYPE"/>
        <result property="picturePath" column="PICTURE_PATH"/>
        <result property="userId" column="USER_ID"/>
        <result property="itemId" column="ITEM_ID"/>
        <result property="operateTime" column="OPERATE_TIME"/>
        <result property="isDelete" column="IS_DELETE"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectAppLogVo">
        select ID,
               MODIFYDATE,
               CREATEDATE,
               OPERATE_TYPE,
               PICTURE_PATH,
               USER_ID,
               ITEM_ID,
               OPERATE_TIME,
               IS_DELETE
        from APP_LOG
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="AppLogMap">
        <include refid="selectAppLogVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="AppLogMap">
        <include refid="selectAppLogVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

