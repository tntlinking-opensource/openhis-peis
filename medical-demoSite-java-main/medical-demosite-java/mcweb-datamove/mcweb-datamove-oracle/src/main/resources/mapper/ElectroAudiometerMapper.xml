        <result property="shortCode" column="SHORT_CODE"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectElectroAudiometerVo">
        select DEP_ID,
               ID,
               ID_EXAMTYPE,
               PATIENTCODE,
               CREATEDATE,
               MODIFYDATE,
               BONE_LEFT250,
               AIR_LEFT125,
               AIR_RIGHT125,
               BONE_RIGHT250,
               BONE_LEFT4000,
               BONE_RIGHT4000,
               BONE_LEFT2000,
               BONE_RIGHT2000,
               BONE_LEFT1000,
               BONE_RIGHT1000,
               BONE_LEFT500,
               BONE_RIGHT500,
               AIR_LEFT250,
               AIR_RIGHT250,
               AIR_LEFT500,
               AIR_RIGHT500,
               AIR_LEFT1000,
               AIR_RIGHT1000,
               AIR_LEFT2000,
               AIR_RIGHT2000,
               AIR_LEFT4000,
               AIR_RIGHT4000,
               AIR_LEFT8000,
               AIR_RIGHT8000,
               AIR_LEFT3000,
               AIR_RIGHT3000,
               AIR_LEFT6000, DESCRIBE, TEST_RESULT, BONE_LEFT3000, AIR_RIGHT6000, BONE_RIGHT3000, AIR_IMG_PATH, BONE_IMG_PATH, SHORT_CODE
        from ELECTRO_AUDIOMETER
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="ElectroAudiometerMap">
        <include refid="selectElectroAudiometerVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="ElectroAudiometerMap">
        <include refid="selectElectroAudiometerVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

