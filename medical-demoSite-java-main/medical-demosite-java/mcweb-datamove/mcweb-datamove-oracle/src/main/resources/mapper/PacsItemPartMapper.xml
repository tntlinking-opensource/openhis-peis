<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.oracle.dao.PacsItemPartMapper">

    <resultMap type="com.center.medical.datamove.oracle.bean.model.PacsItemPart" id="PacsItemPartMap">
        <id property="id" column="ID"/>
        <result property="createdate" column="CREATEDATE"/>
        <result property="modifydate" column="MODIFYDATE"/>
        <result property="itemId" column="ITEM_ID"/>
        <result property="partId" column="PART_ID"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectPacsItemPartVo">
        select ID, CREATEDATE, MODIFYDATE, ITEM_ID, PART_ID
        from PACS_ITEM_PART
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="PacsItemPartMap">
        <include refid="selectPacsItemPartVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="PacsItemPartMap">
        <include refid="selectPacsItemPartVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

