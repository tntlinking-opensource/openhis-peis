<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.oracle.dao.SellOutsideMapper">

    <resultMap type="com.center.medical.datamove.oracle.bean.model.SellOutside" id="SellOutsideMap">
        <id property="id" column="ID"/>
        <result property="fzx" column="FZX"/>
        <result property="kh" column="KH"/>
        <result property="allprice" column="ALLPRICE"/>
        <result property="zk" column="ZK"/>
        <result property="sj" column="SJ"/>
        <result property="items" column="ITEMS"/>
        <result property="sellman" column="SELLMAN"/>
        <result property="createdate" column="CREATEDATE"/>
        <result property="modifydate" column="MODIFYDATE"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectSellOutsideVo">
        select ID,
               FZX,
               KH,
               ALLPRICE,
               ZK,
               SJ,
               ITEMS,
               SELLMAN,
               CREATEDATE,
               MODIFYDATE
        from SELL_OUTSIDE
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="SellOutsideMap">
        <include refid="selectSellOutsideVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="SellOutsideMap">
        <include refid="selectSellOutsideVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

