<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.oracle.dao.InspectReportMapper">

    <resultMap type="com.center.medical.datamove.oracle.bean.model.InspectReport" id="InspectReportMap">
        <id property="id" column="ID"/>
        <result property="createdate" column="CREATEDATE"/>
        <result property="modifydate" column="MODIFYDATE"/>
        <result property="patientcode" column="PATIENTCODE"/>
        <result property="printTime" column="PRINT_TIME"/>
        <result property="printCount" column="PRINT_COUNT"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectInspectReportVo">
        select ID, CREATEDATE, MODIFYDATE, PATIENTCODE, PRINT_TIME, PRINT_COUNT
        from INSPECT_REPORT
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="InspectReportMap">
        <include refid="selectInspectReportVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="InspectReportMap">
        <include refid="selectInspectReportVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

