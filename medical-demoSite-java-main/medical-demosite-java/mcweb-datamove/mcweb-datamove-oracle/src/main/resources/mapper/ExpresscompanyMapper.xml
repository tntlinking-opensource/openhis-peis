<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.oracle.dao.ExpresscompanyMapper">

    <resultMap type="com.center.medical.datamove.oracle.bean.model.Expresscompany" id="ExpresscompanyMap">
        <id property="id" column="ID"/>
        <result property="expressName" column="EXPRESS_NAME"/>
        <result property="expressPhone" column="EXPRESS_PHONE"/>
        <result property="inputCode" column="INPUT_CODE"/>
        <result property="createdate" column="CREATEDATE"/>
        <result property="modifydate" column="MODIFYDATE"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectExpresscompanyVo">
        select ID, EXPRESS_NAME, EXPRESS_PHONE, INPUT_CODE, CREATEDATE, MODIFYDATE
        from EXPRESSCOMPANY
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="ExpresscompanyMap">
        <include refid="selectExpresscompanyVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="ExpresscompanyMap">
        <include refid="selectExpresscompanyVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

