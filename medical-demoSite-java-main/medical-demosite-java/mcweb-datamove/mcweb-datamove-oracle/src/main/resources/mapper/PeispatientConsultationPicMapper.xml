<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.oracle.dao.PeispatientConsultationPicMapper">

    <resultMap type="com.center.medical.datamove.oracle.bean.model.PeispatientConsultationPic"
               id="PeispatientConsultationPicMap">
        <id property="id" column="ID"/>
        <result property="createdate" column="CREATEDATE"/>
        <result property="modifydate" column="MODIFYDATE"/>
        <result property="patientcode" column="PATIENTCODE"/>
        <result property="signPicture" column="SIGN_PICTURE"/>
        <result property="datecreated" column="DATECREATED"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectPeispatientConsultationPicVo">
        select ID, CREATEDATE, MODIFYDATE, PATIENTCODE, SIGN_PICTURE, DATECREATED
        from PEISPATIENT_CONSULTATION_PIC
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="PeispatientConsultationPicMap">
        <include refid="selectPeispatientConsultationPicVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="PeispatientConsultationPicMap">
        <include refid="selectPeispatientConsultationPicVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

