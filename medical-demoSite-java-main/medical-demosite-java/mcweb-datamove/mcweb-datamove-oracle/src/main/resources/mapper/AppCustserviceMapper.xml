<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.oracle.dao.AppCustserviceMapper">

    <resultMap type="com.center.medical.datamove.oracle.bean.model.AppCustservice" id="AppCustserviceMap">
        <id property="id" column="ID"/>
        <result property="createdate" column="CREATEDATE"/>
        <result property="modifydate" column="MODIFYDATE"/>
        <result property="serviceId" column="SERVICE_ID"/>
        <result property="customer" column="CUSTOMER"/>
        <result property="deleteFlg" column="DELETE_FLG"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectAppCustserviceVo">
        select ID, CREATEDATE, MODIFYDATE, SERVICE_ID, CUSTOMER, DELETE_FLG
        from APP_CUSTSERVICE
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="AppCustserviceMap">
        <include refid="selectAppCustserviceVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="AppCustserviceMap">
        <include refid="selectAppCustserviceVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

