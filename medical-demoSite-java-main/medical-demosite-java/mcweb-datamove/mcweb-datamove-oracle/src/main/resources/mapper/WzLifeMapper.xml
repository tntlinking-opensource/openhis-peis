<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.oracle.dao.WzLifeMapper">

    <resultMap type="com.center.medical.datamove.oracle.bean.model.WzLife" id="WzLifeMap">
        <id property="id" column="ID"/>
        <result property="djls" column="DJLS"/>
        <result property="patientcode" column="PATIENTCODE"/>
        <result property="amount" column="AMOUNT"/>
        <result property="id1" column="ID1"/>
        <result property="createDate" column="CREATE_DATE"/>
        <result property="cqshdq" column="CQSHDQ"/>
        <result property="ysxg" column="YSXG"/>
        <result property="qjxg" column="QJXG"/>
        <result property="xgtz" column="XGTZ"/>
        <result property="jszk" column="JSZK"/>
        <result property="dfb" column="DFB"/>
        <result property="yqshs" column="YQSHS"/>
        <result property="ywlyqk" column="YWLYQK"/>
        <result property="dbUser" column="DB_USER"/>
        <result property="remark" column="REMARK"/>
        <result property="createId" column="CREATE_ID"/>
        <result property="modifyId" column="MODIFY_ID"/>
        <result property="createdate" column="CREATEDATE"/>
        <result property="modifydate" column="MODIFYDATE"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectWzLifeVo">
        select DJLS,
               PATIENTCODE,
               AMOUNT,
               ID1,
               CREATE_DATE,
               CQSHDQ,
               YSXG,
               QJXG,
               XGTZ,
               JSZK,
               DFB,
               YQSHS,
               YWLYQK,
               DB_USER,
               REMARK,
               CREATE_ID,
               MODIFY_ID,
               ID,
               CREATEDATE,
               MODIFYDATE
        from WZ_LIFE
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="WzLifeMap">
        <include refid="selectWzLifeVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="WzLifeMap">
        <include refid="selectWzLifeVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

