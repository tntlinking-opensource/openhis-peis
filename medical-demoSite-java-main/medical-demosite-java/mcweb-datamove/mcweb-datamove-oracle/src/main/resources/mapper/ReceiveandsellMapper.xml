<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.oracle.dao.ReceiveandsellMapper">

    <resultMap type="com.center.medical.datamove.oracle.bean.model.Receiveandsell" id="ReceiveandsellMap">
        <id property="id" column="ID"/>
        <result property="clientid" column="CLIENTID"/>
        <result property="xsjlid" column="XSJLID"/>
        <result property="lqrq" column="LQRQ"/>
        <result property="isDelete" column="IS_DELETE"/>
        <result property="czzt" column="CZZT"/>
        <result property="createdate" column="CREATEDATE"/>
        <result property="modifydate" column="MODIFYDATE"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectReceiveandsellVo">
        select ID,
               CLIENTID,
               XSJLID,
               LQRQ,
               IS_DELETE,
               CZZT,
               CREATEDATE,
               MODIFYDATE
        from RECEIVEANDSELL
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="ReceiveandsellMap">
        <include refid="selectReceiveandsellVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="ReceiveandsellMap">
        <include refid="selectReceiveandsellVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

