<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.oracle.dao.AppFaqMapper">

    <resultMap type="com.center.medical.datamove.oracle.bean.model.AppFaq" id="AppFaqMap">
        <id property="id" column="ID"/>
        <result property="createdate" column="CREATEDATE"/>
        <result property="modifydate" column="MODIFYDATE"/>
        <result property="title" column="TITLE"/>
        <result property="content" column="CONTENT"/>
        <result property="isDelete" column="IS_DELETE"/>
        <result property="idx" column="IDX"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectAppFaqVo">
        select ID, CREATEDATE, MODIFYDATE, TITLE, CONTENT, IS_DELETE, IDX
        from APP_FAQ
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="AppFaqMap">
        <include refid="selectAppFaqVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="AppFaqMap">
        <include refid="selectAppFaqVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

