<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.oracle.dao.TempFeeitemMapper">

    <resultMap type="com.center.medical.datamove.oracle.bean.model.TempFeeitem" id="TempFeeitemMap">
        <id property="id" column="ID"/>
        <result property="createdate" column="CREATEDATE"/>
        <result property="modifydate" column="MODIFYDATE"/>
        <result property="itemId" column="ITEM_ID"/>
        <result property="doctorUsername" column="DOCTOR_USERNAME"/>
        <result property="price" column="PRICE"/>
        <result property="remarks" column="REMARKS"/>
        <result property="patientcode" column="PATIENTCODE"/>
        <result property="isDelete" column="IS_DELETE"/>
        <result property="feeitemId" column="FEEITEM_ID"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectTempFeeitemVo">
        select ID,
               CREATEDATE,
               MODIFYDATE,
               ITEM_ID,
               DOCTOR_USERNAME,
               PRICE,
               REMARKS,
               PATIENTCODE,
               IS_DELETE,
               FEEITEM_ID
        from TEMP_FEEITEM
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="TempFeeitemMap">
        <include refid="selectTempFeeitemVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="TempFeeitemMap">
        <include refid="selectTempFeeitemVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

