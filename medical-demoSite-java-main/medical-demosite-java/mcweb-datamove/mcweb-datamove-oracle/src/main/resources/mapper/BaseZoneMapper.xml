<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.oracle.dao.BaseZoneMapper">

    <resultMap type="com.center.medical.datamove.oracle.bean.model.BaseZone" id="BaseZoneMap">
        <id property="id" column="ID"/>
        <result property="createdate" column="CREATEDATE"/>
        <result property="modifydate" column="MODIFYDATE"/>
        <result property="zoneCode" column="ZONE_CODE"/>
        <result property="zoneName" column="ZONE_NAME"/>
        <result property="zoneLevel" column="ZONE_LEVEL"/>
        <result property="pcode" column="PCODE"/>
        <result property="isDelete" column="IS_DELETE"/>
        <result property="qingdaoCode" column="QINGDAO_CODE"/>
        <result property="qingdaoPcode" column="QINGDAO_PCODE"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectBaseZoneVo">
        select ID,
               CREATEDATE,
               MODIFYDATE,
               ZONE_CODE,
               ZONE_NAME,
               ZONE_LEVEL,
               PCODE,
               IS_DELETE,
               QINGDAO_CODE,
               QINGDAO_PCODE
        from BASE_ZONE
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="BaseZoneMap">
        <include refid="selectBaseZoneVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="BaseZoneMap">
        <include refid="selectBaseZoneVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

