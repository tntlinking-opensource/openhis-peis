<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.oracle.dao.BaseZoneQdMapper">

    <resultMap type="com.center.medical.datamove.oracle.bean.model.BaseZoneQd" id="BaseZoneQdMap">
        <id property="id" column="ID"/>
        <result property="createdate" column="CREATEDATE"/>
        <result property="modifydate" column="MODIFYDATE"/>
        <result property="zoneCode" column="ZONE_CODE"/>
        <result property="zoneName" column="ZONE_NAME"/>
        <result property="qingdaoZoneCode" column="QINGDAO_ZONE_CODE"/>
        <result property="qingdaoZoneName" column="QINGDAO_ZONE_NAME"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectBaseZoneQdVo">
        select ID, CREATEDATE, MODIFYDATE, ZONE_CODE, ZONE_NAME, QINGDAO_ZONE_CODE, QINGDAO_ZONE_NAME
        from BASE_ZONE_QD
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="BaseZoneQdMap">
        <include refid="selectBaseZoneQdVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="BaseZoneQdMap">
        <include refid="selectBaseZoneQdVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

