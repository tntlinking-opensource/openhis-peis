<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.oracle.dao.CustFeedbackMapper">

    <resultMap type="com.center.medical.datamove.oracle.bean.model.CustFeedback" id="CustFeedbackMap">
        <id property="id" column="ID"/>
        <result property="createdate" column="CREATEDATE"/>
        <result property="modifydate" column="MODIFYDATE"/>
        <result property="name" column="NAME"/>
        <result property="type" column="TYPE"/>
        <result property="contact" column="CONTACT"/>
        <result property="phone" column="PHONE"/>
        <result property="detail" column="DETAIL"/>
        <result property="faultdate" column="FAULTDATE"/>
        <result property="submited" column="SUBMITED"/>
        <result property="state" column="STATE"/>
        <result property="submitDate" column="SUBMIT_DATE"/>
        <result property="submitId" column="SUBMIT_ID"/>
        <result property="submitName" column="SUBMIT_NAME"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectCustFeedbackVo">
        select ID,
               CREATEDATE,
               MODIFYDATE,
               NAME,
               TYPE,
               CONTACT,
               PHONE,
               DETAIL,
               FAULTDATE,
               SUBMITED,
               STATE,
               SUBMIT_DATE,
               SUBMIT_ID,
               SUBMIT_NAME
        from CUST_FEEDBACK
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="CustFeedbackMap">
        <include refid="selectCustFeedbackVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="CustFeedbackMap">
        <include refid="selectCustFeedbackVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

