<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.oracle.dao.FxPositiveMapper">

    <resultMap type="com.center.medical.datamove.oracle.bean.model.FxPositive" id="FxPositiveMap">
        <id property="id" column="ID"/>
        <result property="sampleId" column="SAMPLE_ID"/>
        <result property="createdate" column="CREATEDATE"/>
        <result property="modifydate" column="MODIFYDATE"/>
        <result property="dateregister" column="DATEREGISTER"/>
        <result property="idOrg" column="ID_ORG"/>
        <result property="orgName" column="ORG_NAME"/>
        <result property="orgDepart" column="ORG_DEPART"/>
        <result property="patientcode" column="PATIENTCODE"/>
        <result property="patientname" column="PATIENTNAME"/>
        <result property="sex" column="SEX"/>
        <result property="age" column="AGE"/>
        <result property="positiveResult" column="POSITIVE_RESULT"/>
        <result property="verdict" column="VERDICT"/>
        <result property="offer" column="OFFER"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectFxPositiveVo">
        select ID,
               SAMPLE_ID,
               CREATEDATE,
               MODIFYDATE,
               DATEREGISTER,
               ID_ORG,
               ORG_NAME,
               ORG_DEPART,
               PATIENTCODE,
               PATIENTNAME,
               SEX,
               AGE,
               POSITIVE_RESULT,
               VERDICT,
               OFFER
        from FX_POSITIVE
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="FxPositiveMap">
        <include refid="selectFxPositiveVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="FxPositiveMap">
        <include refid="selectFxPositiveVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

