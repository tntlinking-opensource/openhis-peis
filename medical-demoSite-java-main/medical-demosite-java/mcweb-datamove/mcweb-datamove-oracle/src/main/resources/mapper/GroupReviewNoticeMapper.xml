<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.oracle.dao.GroupReviewNoticeMapper">

    <resultMap type="com.center.medical.datamove.oracle.bean.model.GroupReviewNotice" id="GroupReviewNoticeMap">
        <id property="id" column="ID"/>
        <result property="createdate" column="CREATEDATE"/>
        <result property="modifydate" column="MODIFYDATE"/>
        <result property="startDate" column="START_DATE"/>
        <result property="endDate" column="END_DATE"/>
        <result property="customerId" column="CUSTOMER_ID"/>
        <result property="creator" column="CREATOR"/>
        <result property="status" column="STATUS"/>
        <result property="errorMsg" column="ERROR_MSG"/>
        <result property="configId" column="CONFIG_ID"/>
        <result property="url" column="URL"/>
        <result property="ddh" column="DDH"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectGroupReviewNoticeVo">
        select ID,
               CREATEDATE,
               MODIFYDATE,
               START_DATE,
               END_DATE,
               CUSTOMER_ID,
               CREATOR,
               STATUS,
               ERROR_MSG,
               CONFIG_ID,
               URL,
               DDH
        from GROUP_REVIEW_NOTICE
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="GroupReviewNoticeMap">
        <include refid="selectGroupReviewNoticeVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="GroupReviewNoticeMap">
        <include refid="selectGroupReviewNoticeVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

