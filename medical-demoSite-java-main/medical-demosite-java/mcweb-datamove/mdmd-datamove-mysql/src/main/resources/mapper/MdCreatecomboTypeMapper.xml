<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.common.dao.MdCreatecomboTypeMapper">

    <resultMap type="com.center.medical.datamove.common.bean.model.MdCreatecomboType" id="MdCreatecomboTypeMap">
        <id property="id" column="id"/>
        <result property="createdate" column="createdate"/>
        <result property="modifydate" column="modifydate"/>
        <result property="name" column="name"/>
        <result property="typeIndex" column="type_index"/>
        <result property="typeState" column="type_state"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectMdCreatecomboTypeVo">
        select id, createdate, modifydate, name, type_index, type_state
        from md_createcombo_type
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="MdCreatecomboTypeMap">
        <include refid="selectMdCreatecomboTypeVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="MdCreatecomboTypeMap">
        <include refid="selectMdCreatecomboTypeVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

