<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.common.dao.MdInspectReportMapper">

    <resultMap type="com.center.medical.datamove.common.bean.model.MdInspectReport" id="MdInspectReportMap">
        <id property="id" column="id"/>
        <result property="createdate" column="createdate"/>
        <result property="modifydate" column="modifydate"/>
        <result property="patientcode" column="patientcode"/>
        <result property="printTime" column="print_time"/>
        <result property="printCount" column="print_count"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectMdInspectReportVo">
        select id, createdate, modifydate, patientcode, print_time, print_count
        from md_inspect_report
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="MdInspectReportMap">
        <include refid="selectMdInspectReportVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="MdInspectReportMap">
        <include refid="selectMdInspectReportVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

