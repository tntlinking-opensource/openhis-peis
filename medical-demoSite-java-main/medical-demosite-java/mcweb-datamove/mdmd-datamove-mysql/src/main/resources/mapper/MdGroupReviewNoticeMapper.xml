<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.common.dao.MdGroupReviewNoticeMapper">

    <resultMap type="com.center.medical.datamove.common.bean.model.MdGroupReviewNotice" id="MdGroupReviewNoticeMap">
        <id property="id" column="id"/>
        <result property="createdate" column="createdate"/>
        <result property="modifydate" column="modifydate"/>
        <result property="startDate" column="start_date"/>
        <result property="endDate" column="end_date"/>
        <result property="customerId" column="customer_id"/>
        <result property="creator" column="creator"/>
        <result property="status" column="status"/>
        <result property="errorMsg" column="error_msg"/>
        <result property="configId" column="config_id"/>
        <result property="url" column="url"/>
        <result property="ddh" column="ddh"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectMdGroupReviewNoticeVo">
        select id,
               createdate,
               modifydate,
               start_date,
               end_date,
               customer_id,
               creator,
               status,
               error_msg,
               config_id,
               url,
               ddh
        from md_group_review_notice
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="MdGroupReviewNoticeMap">
        <include refid="selectMdGroupReviewNoticeVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="MdGroupReviewNoticeMap">
        <include refid="selectMdGroupReviewNoticeVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

