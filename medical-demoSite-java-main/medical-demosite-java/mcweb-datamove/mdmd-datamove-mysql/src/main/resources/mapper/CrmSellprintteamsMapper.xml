<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.common.dao.CrmSellprintteamsMapper">

    <resultMap type="com.center.medical.datamove.common.bean.model.CrmSellprintteams" id="CrmSellprintteamsMap">
        <id property="id" column="id"/>
        <result property="dyxmflmc" column="dyxmflmc"/>
        <result property="srm" column="srm"/>
        <result property="bz" column="bz"/>
        <result property="createdate" column="createdate"/>
        <result property="modifydate" column="modifydate"/>
        <result property="isDelete" column="is_delete"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectCrmSellprintteamsVo">
        select id, dyxmflmc, srm, bz, createdate, modifydate, is_delete
        from crm_sellprintteams
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="CrmSellprintteamsMap">
        <include refid="selectCrmSellprintteamsVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="CrmSellprintteamsMap">
        <include refid="selectCrmSellprintteamsVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

