<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.common.dao.QrtzLocksMapper">

    <resultMap type="com.center.medical.datamove.common.bean.model.QrtzLocks" id="QrtzLocksMap">
        <id property="schedName" column="sched_name"/>
        <result property="lockName" column="lock_name"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectQrtzLocksVo">
        select sched_name, lock_name
        from qrtz_locks
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="QrtzLocksMap">
        <include refid="selectQrtzLocksVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="QrtzLocksMap">
        <include refid="selectQrtzLocksVo"/>
        <where>
            schedName = #{id}
        </where>
    </select>

</mapper>

