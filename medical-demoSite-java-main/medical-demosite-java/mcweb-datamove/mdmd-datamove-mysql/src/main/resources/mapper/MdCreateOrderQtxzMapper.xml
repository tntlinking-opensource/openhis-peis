<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.common.dao.MdCreateOrderQtxzMapper">

    <resultMap type="com.center.medical.datamove.common.bean.model.MdCreateOrderQtxz" id="MdCreateOrderQtxzMap">
        <id property="id" column="id"/>
        <result property="createdate" column="createdate"/>
        <result property="modifydate" column="modifydate"/>
        <result property="orderId" column="order_id"/>
        <result property="username" column="username"/>
        <result property="qtxz" column="qtxz"/>
        <result property="oldQtxz" column="old_qtxz"/>
        <result property="idx" column="idx"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectMdCreateOrderQtxzVo">
        select id,
               createdate,
               modifydate,
               order_id,
               username,
               qtxz,
               old_qtxz,
               idx
        from md_create_order_qtxz
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="MdCreateOrderQtxzMap">
        <include refid="selectMdCreateOrderQtxzVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="MdCreateOrderQtxzMap">
        <include refid="selectMdCreateOrderQtxzVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

