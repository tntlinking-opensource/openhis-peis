<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.common.dao.SysJobLogMapper">

    <resultMap type="com.center.medical.datamove.common.bean.model.SysJobLog" id="SysJobLogMap">
        <id property="jobLogId" column="job_log_id"/>
        <result property="jobName" column="job_name"/>
        <result property="jobGroup" column="job_group"/>
        <result property="invokeTarget" column="invoke_target"/>
        <result property="jobMessage" column="job_message"/>
        <result property="status" column="status"/>
        <result property="exceptionInfo" column="exception_info"/>
        <result property="createTime" column="create_time"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectSysJobLogVo">
        select job_log_id,
               job_name,
               job_group,
               invoke_target,
               job_message,
               status,
               exception_info,
               create_time
        from sys_job_log
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="SysJobLogMap">
        <include refid="selectSysJobLogVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="SysJobLogMap">
        <include refid="selectSysJobLogVo"/>
        <where>
            jobLogId = #{id}
        </where>
    </select>

</mapper>

