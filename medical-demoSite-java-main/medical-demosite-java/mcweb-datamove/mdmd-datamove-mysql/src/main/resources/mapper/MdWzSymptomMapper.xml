<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.common.dao.MdWzSymptomMapper">

    <resultMap type="com.center.medical.datamove.common.bean.model.MdWzSymptom" id="MdWzSymptomMap">
        <id property="id" column="id"/>
        <result property="patientcode" column="patientcode"/>
        <result property="symptomId" column="symptom_id"/>
        <result property="createdate" column="createdate"/>
        <result property="modifydate" column="modifydate"/>
        <result property="createId" column="create_id"/>
        <result property="modifyId" column="modify_id"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectMdWzSymptomVo">
        select id, patientcode, symptom_id, createdate, modifydate, create_id, modify_id
        from md_wz_symptom
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="MdWzSymptomMap">
        <include refid="selectMdWzSymptomVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="MdWzSymptomMap">
        <include refid="selectMdWzSymptomVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

