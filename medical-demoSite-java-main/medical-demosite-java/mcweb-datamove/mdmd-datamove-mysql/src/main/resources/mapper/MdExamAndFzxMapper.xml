<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.common.dao.MdExamAndFzxMapper">

    <resultMap type="com.center.medical.datamove.common.bean.model.MdExamAndFzx" id="MdExamAndFzxMap">
        <id property="id" column="id"/>
        <result property="createdate" column="createdate"/>
        <result property="modifydate" column="modifydate"/>
        <result property="examId" column="exam_id"/>
        <result property="fzxId" column="fzx_id"/>
        <result property="tbzt" column="tbzt"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectMdExamAndFzxVo">
        select id, createdate, modifydate, exam_id, fzx_id, tbzt
        from md_exam_and_fzx
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="MdExamAndFzxMap">
        <include refid="selectMdExamAndFzxVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="MdExamAndFzxMap">
        <include refid="selectMdExamAndFzxVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

