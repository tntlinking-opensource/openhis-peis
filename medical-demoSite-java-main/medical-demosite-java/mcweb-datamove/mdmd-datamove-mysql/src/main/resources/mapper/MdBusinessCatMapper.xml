<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.common.dao.MdBusinessCatMapper">

    <resultMap type="com.center.medical.datamove.common.bean.model.MdBusinessCat" id="MdBusinessCatMap">
        <id property="typeId" column="type_id"/>
        <result property="parentId" column="parent_id"/>
        <result property="typeName" column="type_name"/>
        <result property="inputCode" column="input_code"/>
        <result property="icon" column="icon"/>
        <result property="tip" column="tip"/>
        <result property="seq" column="seq"/>
        <result property="status" column="status"/>
        <result property="grade" column="grade"/>
        <result property="createdate" column="createdate"/>
        <result property="modifydate" column="modifydate"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectMdBusinessCatVo">
        select type_id,
               parent_id,
               type_name,
               input_code,
               icon,
               tip,
               seq,
               status,
               grade,
               createdate,
               modifydate
        from md_business_cat
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="MdBusinessCatMap">
        <include refid="selectMdBusinessCatVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="MdBusinessCatMap">
        <include refid="selectMdBusinessCatVo"/>
        <where>
            typeId = #{id}
        </where>
    </select>

</mapper>

