<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.common.dao.MdOrdersummaryMapper">

    <resultMap type="com.center.medical.datamove.common.bean.model.MdOrdersummary" id="MdOrdersummaryMap">
        <id property="id" column="id"/>
        <result property="ddid" column="ddid"/>
        <result property="ddmc" column="ddmc"/>
        <result property="khdwmc" column="khdwmc"/>
        <result property="xsjl" column="xsjl"/>
        <result property="zj" column="zj"/>
        <result property="xsjlid" column="xsjlid"/>
        <result property="fzxid" column="fzxid"/>
        <result property="isDelete" column="is_delete"/>
        <result property="createdate" column="createdate"/>
        <result property="modifydate" column="modifydate"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectMdOrdersummaryVo">
        select id,
               ddid,
               ddmc,
               khdwmc,
               xsjl,
               zj,
               xsjlid,
               fzxid,
               is_delete,
               createdate,
               modifydate
        from md_ordersummary
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="MdOrdersummaryMap">
        <include refid="selectMdOrdersummaryVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="MdOrdersummaryMap">
        <include refid="selectMdOrdersummaryVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

