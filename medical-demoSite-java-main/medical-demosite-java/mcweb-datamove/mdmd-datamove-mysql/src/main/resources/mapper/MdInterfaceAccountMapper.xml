<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.common.dao.MdInterfaceAccountMapper">

    <resultMap type="com.center.medical.datamove.common.bean.model.MdInterfaceAccount" id="MdInterfaceAccountMap">
        <id property="id" column="id"/>
        <result property="createdate" column="createdate"/>
        <result property="modifydate" column="modifydate"/>
        <result property="username" column="username"/>
        <result property="password" column="password"/>
        <result property="key" column="key"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectMdInterfaceAccountVo">
        select id, createdate, modifydate, username, password, key
        from md_interface_account
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="MdInterfaceAccountMap">
        <include refid="selectMdInterfaceAccountVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="MdInterfaceAccountMap">
        <include refid="selectMdInterfaceAccountVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

