<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.common.dao.MdWzZybsMapper">

    <resultMap type="com.center.medical.datamove.common.bean.model.MdWzZybs" id="MdWzZybsMap">
        <id property="id" column="id"/>
        <result property="djls" column="djls"/>
        <result property="idPatientarchive" column="id_patientarchive"/>
        <result property="amount" column="amount"/>
        <result property="createDate" column="create_date"/>
        <result property="occupationDiseast" column="occupation_diseast"/>
        <result property="diagnosisDate" column="diagnosis_date"/>
        <result property="diagnosisDept" column="diagnosis_dept"/>
        <result property="diagnosisDesc" column="diagnosis_desc"/>
        <result property="dbUser" column="db_user"/>
        <result property="remark" column="remark"/>
        <result property="createdate" column="createdate"/>
        <result property="modifydate" column="modifydate"/>
        <result property="createId" column="create_id"/>
        <result property="modifyId" column="modify_id"/>
        <result property="status" column="status"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectMdWzZybsVo">
        select id,
               djls,
               id_patientarchive,
               amount,
               create_date,
               occupation_diseast,
               diagnosis_date,
               diagnosis_dept,
               diagnosis_desc,
               db_user,
               remark,
               createdate,
               modifydate,
               create_id,
               modify_id,
               status
        from md_wz_zybs
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="MdWzZybsMap">
        <include refid="selectMdWzZybsVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="MdWzZybsMap">
        <include refid="selectMdWzZybsVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

