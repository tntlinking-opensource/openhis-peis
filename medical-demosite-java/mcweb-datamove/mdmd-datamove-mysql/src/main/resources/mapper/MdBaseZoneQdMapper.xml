<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.common.dao.MdBaseZoneQdMapper">

    <resultMap type="com.center.medical.datamove.common.bean.model.MdBaseZoneQd" id="MdBaseZoneQdMap">
        <id property="id" column="id"/>
        <result property="createdate" column="createdate"/>
        <result property="modifydate" column="modifydate"/>
        <result property="zoneCode" column="zone_code"/>
        <result property="zoneName" column="zone_name"/>
        <result property="qingdaoZoneCode" column="qingdao_zone_code"/>
        <result property="qingdaoZoneName" column="qingdao_zone_name"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectMdBaseZoneQdVo">
        select id, createdate, modifydate, zone_code, zone_name, qingdao_zone_code, qingdao_zone_name
        from md_base_zone_qd
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="MdBaseZoneQdMap">
        <include refid="selectMdBaseZoneQdVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="MdBaseZoneQdMap">
        <include refid="selectMdBaseZoneQdVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

