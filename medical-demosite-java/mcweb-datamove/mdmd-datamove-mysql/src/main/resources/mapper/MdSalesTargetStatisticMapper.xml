<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.common.dao.MdSalesTargetStatisticMapper">

    <resultMap type="com.center.medical.datamove.common.bean.model.MdSalesTargetStatistic"
               id="MdSalesTargetStatisticMap">
        <id property="id" column="id"/>
        <result property="userid" column="userid"/>
        <result property="username" column="username"/>
        <result property="complete" column="complete"/>
        <result property="complete1" column="complete1"/>
        <result property="complete2" column="complete2"/>
        <result property="complete3" column="complete3"/>
        <result property="complete4" column="complete4"/>
        <result property="complete5" column="complete5"/>
        <result property="complete6" column="complete6"/>
        <result property="complete7" column="complete7"/>
        <result property="complete8" column="complete8"/>
        <result property="complete9" column="complete9"/>
        <result property="complete10" column="complete10"/>
        <result property="complete11" column="complete11"/>
        <result property="complete12" column="complete12"/>
        <result property="bz" column="bz"/>
        <result property="cid" column="cid"/>
        <result property="year" column="year"/>
        <result property="type" column="type"/>
        <result property="createdate" column="createdate"/>
        <result property="modifydate" column="modifydate"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectMdSalesTargetStatisticVo">
        select id,
               userid,
               username,
               complete,
               complete1,
               complete2,
               complete3,
               complete4,
               complete5,
               complete6,
               complete7,
               complete8,
               complete9,
               complete10,
               complete11,
               complete12,
               bz,
               cid, year, type, createdate, modifydate
        from md_sales_target_statistic
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="MdSalesTargetStatisticMap">
        <include refid="selectMdSalesTargetStatisticVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="MdSalesTargetStatisticMap">
        <include refid="selectMdSalesTargetStatisticVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

