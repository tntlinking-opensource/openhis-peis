<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.common.dao.SysUploadLogMapper">

    <resultMap type="com.center.medical.datamove.common.bean.model.SysUploadLog" id="SysUploadLogMap">
        <id property="id" column="id"/>
        <result property="createdate" column="createdate"/>
        <result property="modifydate" column="modifydate"/>
        <result property="cid" column="cid"/>
        <result property="uploadType" column="upload_type"/>
        <result property="uploadNum" column="upload_num"/>
        <result property="uploadDate" column="upload_date"/>
        <result property="uploadStatus" column="upload_status"/>
        <result property="msg" column="msg"/>
        <result property="ip" column="ip"/>
        <result property="uploadDateStr" column="upload_date_str"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectSysUploadLogVo">
        select id,
               createdate,
               modifydate,
               cid,
               upload_type,
               upload_num,
               upload_date,
               upload_status,
               msg,
               ip,
               upload_date_str
        from sys_upload_log
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="SysUploadLogMap">
        <include refid="selectSysUploadLogVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="SysUploadLogMap">
        <include refid="selectSysUploadLogVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

