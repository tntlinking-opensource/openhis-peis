<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.common.dao.MdSyncDataTypeMapper">

    <resultMap type="com.center.medical.datamove.common.bean.model.MdSyncDataType" id="MdSyncDataTypeMap">
        <id property="typeId" column="type_id"/>
        <result property="orderNo" column="order_no"/>
        <result property="typeName" column="type_name"/>
        <result property="remark" column="remark"/>
        <result property="createdate" column="createdate"/>
        <result property="modifydate" column="modifydate"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectMdSyncDataTypeVo">
        select type_id, order_no, type_name, remark, createdate, modifydate
        from md_sync_data_type
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="MdSyncDataTypeMap">
        <include refid="selectMdSyncDataTypeVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="MdSyncDataTypeMap">
        <include refid="selectMdSyncDataTypeVo"/>
        <where>
            typeId = #{id}
        </where>
    </select>

</mapper>

