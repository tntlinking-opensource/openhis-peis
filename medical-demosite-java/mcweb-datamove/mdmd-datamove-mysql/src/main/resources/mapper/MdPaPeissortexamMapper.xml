<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.common.dao.MdPaPeissortexamMapper">

    <resultMap type="com.center.medical.datamove.common.bean.model.MdPaPeissortexam" id="MdPaPeissortexamMap">
        <id property="id" column="id"/>
        <result property="createdate" column="createdate"/>
        <result property="modifydate" column="modifydate"/>
        <result property="branchId" column="branch_id"/>
        <result property="areaId" column="area_id"/>
        <result property="sortNum" column="sort_num"/>
        <result property="sortDate" column="sort_date"/>
        <result property="isOpen" column="is_open"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectMdPaPeissortexamVo">
        select id,
               createdate,
               modifydate,
               branch_id,
               area_id,
               sort_num,
               sort_date,
               is_open
        from md_pa_peissortexam
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="MdPaPeissortexamMap">
        <include refid="selectMdPaPeissortexamVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="MdPaPeissortexamMap">
        <include refid="selectMdPaPeissortexamVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

