               nxtjrs,
               vxtjrs,
               ddjg,
               ddzk,
               ddyhj,
               qtxz,
               spzt,
               spyj,
               xsjlid,
               xsjl,
               fzxid,
               is_delete,
               createdate,
               modifydate,
               urls,
               bgzt,
               xsdh,
               tjxs,
               clurls,
               clspzt,
               clspyj,
               id_inforway,
               tjlx,
               spr,
