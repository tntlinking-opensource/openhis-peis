<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.common.dao.QrtzCalendarsMapper">

    <resultMap type="com.center.medical.datamove.common.bean.model.QrtzCalendars" id="QrtzCalendarsMap">
        <id property="schedName" column="sched_name"/>
        <result property="calendarName" column="calendar_name"/>
        <result property="calendar" column="calendar"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectQrtzCalendarsVo">
        select sched_name, calendar_name, calendar
        from qrtz_calendars
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="QrtzCalendarsMap">
        <include refid="selectQrtzCalendarsVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="QrtzCalendarsMap">
        <include refid="selectQrtzCalendarsVo"/>
        <where>
            schedName = #{id}
        </where>
    </select>

</mapper>

