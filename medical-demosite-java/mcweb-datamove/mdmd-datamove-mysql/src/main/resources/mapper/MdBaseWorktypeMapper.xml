<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.common.dao.MdBaseWorktypeMapper">

    <resultMap type="com.center.medical.datamove.common.bean.model.MdBaseWorktype" id="MdBaseWorktypeMap">
        <id property="id" column="id"/>
        <result property="createdate" column="createdate"/>
        <result property="modifydate" column="modifydate"/>
        <result property="typeName" column="type_name"/>
        <result property="qingdaoCode" column="qingdao_code"/>
        <result property="jinanCode" column="jinan_code"/>
        <result property="changshaCode" column="changsha_code"/>
        <result property="isDelete" column="is_delete"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectMdBaseWorktypeVo">
        select id,
               createdate,
               modifydate,
               type_name,
               qingdao_code,
               jinan_code,
               changsha_code,
               is_delete
        from md_base_worktype
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="MdBaseWorktypeMap">
        <include refid="selectMdBaseWorktypeVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="MdBaseWorktypeMap">
        <include refid="selectMdBaseWorktypeVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

