<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.common.dao.MdOccupationDiseastMapper">

    <resultMap type="com.center.medical.datamove.common.bean.model.MdOccupationDiseast" id="MdOccupationDiseastMap">
        <id property="id" column="id"/>
        <result property="occupationDiseastCode" column="occupation_diseast_code"/>
        <result property="occupationDiseast" column="occupation_diseast"/>
        <result property="inputCode" column="input_code"/>
        <result property="dbUser" column="db_user"/>
        <result property="occupationDiseastClass" column="occupation_diseast_class"/>
        <result property="createdate" column="createdate"/>
        <result property="modifydate" column="modifydate"/>
        <result property="isDelete" column="is_delete"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectMdOccupationDiseastVo">
        select id,
               occupation_diseast_code,
               occupation_diseast,
               input_code,
               db_user,
               occupation_diseast_class,
               createdate,
               modifydate,
               is_delete
        from md_occupation_diseast
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="MdOccupationDiseastMap">
        <include refid="selectMdOccupationDiseastVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="MdOccupationDiseastMap">
        <include refid="selectMdOccupationDiseastVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

