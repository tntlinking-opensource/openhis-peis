<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.common.dao.MdNucleinMapper">

    <resultMap type="com.center.medical.datamove.common.bean.model.MdNuclein" id="MdNucleinMap">
        <id property="id" column="id"/>
        <result property="createdate" column="createdate"/>
        <result property="modifydate" column="modifydate"/>
        <result property="patientcode" column="patientcode"/>
        <result property="creater" column="creater"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectMdNucleinVo">
        select id, createdate, modifydate, patientcode, creater
        from md_nuclein
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="MdNucleinMap">
        <include refid="selectMdNucleinVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="MdNucleinMap">
        <include refid="selectMdNucleinVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

