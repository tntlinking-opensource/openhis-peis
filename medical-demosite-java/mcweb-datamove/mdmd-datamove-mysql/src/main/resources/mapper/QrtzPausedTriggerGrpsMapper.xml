<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.common.dao.QrtzPausedTriggerGrpsMapper">

    <resultMap type="com.center.medical.datamove.common.bean.model.QrtzPausedTriggerGrps"
               id="QrtzPausedTriggerGrpsMap">
        <id property="schedName" column="sched_name"/>
        <result property="triggerGroup" column="trigger_group"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectQrtzPausedTriggerGrpsVo">
        select sched_name, trigger_group
        from qrtz_paused_trigger_grps
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="QrtzPausedTriggerGrpsMap">
        <include refid="selectQrtzPausedTriggerGrpsVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="QrtzPausedTriggerGrpsMap">
        <include refid="selectQrtzPausedTriggerGrpsVo"/>
        <where>
            schedName = #{id}
        </where>
    </select>

</mapper>

