<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.common.dao.MdUpperowerMapper">

    <resultMap type="com.center.medical.datamove.common.bean.model.MdUpperower" id="MdUpperowerMap">
        <id property="id" column="id"/>
        <result property="rymc" column="rymc"/>
        <result property="zw" column="zw"/>
        <result property="xb" column="xb"/>
        <result property="nl" column="nl"/>
        <result property="dh" column="dh"/>
        <result property="sfysj" column="sfysj"/>
        <result property="sj" column="sj"/>
        <result property="createdate" column="createdate"/>
        <result property="modifydate" column="modifydate"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectMdUpperowerVo">
        select id,
               rymc,
               zw,
               xb,
               nl,
               dh,
               sfysj,
               sj,
               createdate,
               modifydate
        from md_upperower
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="MdUpperowerMap">
        <include refid="selectMdUpperowerVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="MdUpperowerMap">
        <include refid="selectMdUpperowerVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

