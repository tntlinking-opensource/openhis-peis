               PATIENTCODE,
               CREATE_DATE,
               CORP_CODE,
               CORP_NAME,
               CORP_ADDRESS,
               POST,
               ECONOMY_TYPE,
               CALLING,
               CALLING_SPACE,
               PHONE,
               FAX,
               DB_USER,
               REMARK,
               LXR,
               CREATEDATE,
               MODIFYDATE,
