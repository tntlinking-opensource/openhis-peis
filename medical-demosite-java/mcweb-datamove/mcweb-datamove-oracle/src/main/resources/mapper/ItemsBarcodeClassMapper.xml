<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.oracle.dao.ItemsBarcodeClassMapper">

    <resultMap type="com.center.medical.datamove.oracle.bean.model.ItemsBarcodeClass" id="ItemsBarcodeClassMap">
        <id property="id" column="ID"/>
        <result property="createdate" column="CREATEDATE"/>
        <result property="modifydate" column="MODIFYDATE"/>
        <result property="className" column="CLASS_NAME"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectItemsBarcodeClassVo">
        select ID, CREATEDATE, MODIFYDATE, CLASS_NAME
        from ITEMS_BARCODE_CLASS
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="ItemsBarcodeClassMap">
        <include refid="selectItemsBarcodeClassVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="ItemsBarcodeClassMap">
        <include refid="selectItemsBarcodeClassVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

