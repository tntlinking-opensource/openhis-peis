<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.oracle.dao.TemporaryqueueMapper">

    <resultMap type="com.center.medical.datamove.oracle.bean.model.Temporaryqueue" id="TemporaryqueueMap">
        <id property="id" column="ID"/>
        <result property="patientcode" column="PATIENTCODE"/>
        <result property="transmitting" column="TRANSMITTING"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectTemporaryqueueVo">
        select ID, PATIENTCODE, TRANSMITTING
        from TEMPORARYQUEUE
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="TemporaryqueueMap">
        <include refid="selectTemporaryqueueVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="TemporaryqueueMap">
        <include refid="selectTemporaryqueueVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

