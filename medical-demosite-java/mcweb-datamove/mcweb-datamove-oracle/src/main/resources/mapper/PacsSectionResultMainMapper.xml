<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.oracle.dao.PacsSectionResultMainMapper">

    <resultMap type="com.center.medical.datamove.oracle.bean.model.PacsSectionResultMain"
               id="PacsSectionResultMainMap">
        <id property="id" column="ID"/>
        <result property="depId" column="DEP_ID"/>
        <result property="rummagerId" column="RUMMAGER_ID"/>
        <result property="writeId" column="WRITE_ID"/>
        <result property="rummagerTime" column="RUMMAGER_TIME"/>
        <result property="writeTime" column="WRITE_TIME"/>
        <result property="createdate" column="CREATEDATE"/>
        <result property="modifydate" column="MODIFYDATE"/>
        <result property="isAudit" column="IS_AUDIT"/>
        <result property="auditId" column="AUDIT_ID"/>
        <result property="auditTime" column="AUDIT_TIME"/>
        <result property="isDanager" column="IS_DANAGER"/>
        <result property="danagerLevel" column="DANAGER_LEVEL"/>
        <result property="conclusions" column="CONCLUSIONS"/>
        <result property="isDelete" column="IS_DELETE"/>
        <result property="auditName" column="AUDIT_NAME"/>
        <result property="rummagerName" column="RUMMAGER_NAME"/>
        <result property="patientcode" column="PATIENTCODE"/>
        <result property="isFinish" column="IS_FINISH"/>
        <result property="zyConclusions" column="ZY_CONCLUSIONS"/>
        <result property="associativeTable" column="ASSOCIATIVE_TABLE"/>
        <result property="shortCode" column="SHORT_CODE"/>
        <result property="fileType" column="FILE_TYPE"/>
        <result property="filePath" column="FILE_PATH"/>
        <result property="description" column="DESCRIPTION"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectPacsSectionResultMainVo">
        select ID,
               DEP_ID,
               RUMMAGER_ID,
               WRITE_ID,
               RUMMAGER_TIME,
               WRITE_TIME,
               CREATEDATE,
               MODIFYDATE,
               IS_AUDIT,
               AUDIT_ID,
               AUDIT_TIME,
               IS_DANAGER,
               DANAGER_LEVEL,
               CONCLUSIONS,
               IS_DELETE,
               AUDIT_NAME,
               RUMMAGER_NAME,
               PATIENTCODE,
               IS_FINISH,
               ZY_CONCLUSIONS,
               ASSOCIATIVE_TABLE,
               SHORT_CODE,
               FILE_TYPE,
               FILE_PATH,
               DESCRIPTION
        from PACS_SECTION_RESULT_MAIN
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="PacsSectionResultMainMap">
        <include refid="selectPacsSectionResultMainVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="PacsSectionResultMainMap">
        <include refid="selectPacsSectionResultMainVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

