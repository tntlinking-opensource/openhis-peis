<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.oracle.dao.CreatecomboTypeMapper">

    <resultMap type="com.center.medical.datamove.oracle.bean.model.CreatecomboType" id="CreatecomboTypeMap">
        <id property="id" column="ID"/>
        <result property="createdate" column="CREATEDATE"/>
        <result property="modifydate" column="MODIFYDATE"/>
        <result property="name" column="NAME"/>
        <result property="typeIndex" column="TYPE_INDEX"/>
        <result property="typeState" column="TYPE_STATE"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectCreatecomboTypeVo">
        select ID, CREATEDATE, MODIFYDATE, NAME, TYPE_INDEX, TYPE_STATE
        from CREATECOMBO_TYPE
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="CreatecomboTypeMap">
        <include refid="selectCreatecomboTypeVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="CreatecomboTypeMap">
        <include refid="selectCreatecomboTypeVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

