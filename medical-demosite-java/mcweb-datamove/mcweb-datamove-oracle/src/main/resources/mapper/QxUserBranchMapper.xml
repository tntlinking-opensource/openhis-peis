<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.oracle.dao.QxUserBranchMapper">

    <resultMap type="com.center.medical.datamove.oracle.bean.model.QxUserBranch" id="QxUserBranchMap">
        <id property="id" column="ID"/>
        <result property="createdate" column="CREATEDATE"/>
        <result property="modifydate" column="MODIFYDATE"/>
        <result property="userId" column="USER_ID"/>
        <result property="cid" column="CID"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectQxUserBranchVo">
        select ID, CREATEDATE, MODIFYDATE, USER_ID, CID
        from QX_USER_BRANCH
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="QxUserBranchMap">
        <include refid="selectQxUserBranchVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="QxUserBranchMap">
        <include refid="selectQxUserBranchVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

