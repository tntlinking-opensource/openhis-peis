<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.oracle.dao.WzJzbMapper">

    <resultMap type="com.center.medical.datamove.oracle.bean.model.WzJzb" id="WzJzbMap">
        <id property="id" column="ID"/>
        <result property="createdate" column="CREATEDATE"/>
        <result property="modifydate" column="MODIFYDATE"/>
        <result property="familyOfDisease" column="FAMILY_OF_DISEASE"/>
        <result property="inputCode" column="INPUT_CODE"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectWzJzbVo">
        select ID, CREATEDATE, MODIFYDATE, FAMILY_OF_DISEASE, INPUT_CODE
        from WZ_JZB
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="WzJzbMap">
        <include refid="selectWzJzbVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="WzJzbMap">
        <include refid="selectWzJzbVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

