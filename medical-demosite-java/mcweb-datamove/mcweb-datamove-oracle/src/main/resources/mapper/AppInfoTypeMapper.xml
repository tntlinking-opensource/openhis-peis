<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.oracle.dao.AppInfoTypeMapper">

    <resultMap type="com.center.medical.datamove.oracle.bean.model.AppInfoType" id="AppInfoTypeMap">
        <id property="id" column="ID"/>
        <result property="createdate" column="CREATEDATE"/>
        <result property="modifydate" column="MODIFYDATE"/>
        <result property="typeName" column="TYPE_NAME"/>
        <result property="typeIndex" column="TYPE_INDEX"/>
        <result property="typeState" column="TYPE_STATE"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectAppInfoTypeVo">
        select ID, CREATEDATE, MODIFYDATE, TYPE_NAME, TYPE_INDEX, TYPE_STATE
        from APP_INFO_TYPE
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="AppInfoTypeMap">
        <include refid="selectAppInfoTypeVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="AppInfoTypeMap">
        <include refid="selectAppInfoTypeVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

