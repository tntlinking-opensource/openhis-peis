<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.datamove.oracle.dao.WzLatestRummagerMapper">

    <resultMap type="com.center.medical.datamove.oracle.bean.model.WzLatestRummager" id="WzLatestRummagerMap">
        <id property="id" column="ID"/>
        <result property="cid" column="CID"/>
        <result property="ksId" column="KS_ID"/>
        <result property="createdate" column="CREATEDATE"/>
        <result property="modifydate" column="MODIFYDATE"/>
        <result property="rummagerId" column="RUMMAGER_ID"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectWzLatestRummagerVo">
        select CID, KS_ID, CREATEDATE, MODIFYDATE, RUMMAGER_ID, ID
        from WZ_LATEST_RUMMAGER
    </sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="WzLatestRummagerMap">
        <include refid="selectWzLatestRummagerVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="WzLatestRummagerMap">
        <include refid="selectWzLatestRummagerVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

