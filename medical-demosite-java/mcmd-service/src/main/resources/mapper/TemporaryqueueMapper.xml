<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.dao.TemporaryqueueMapper">

    <resultMap type="com.center.medical.bean.model.Temporaryqueue" id="TemporaryqueueMap">
        <id property="id" column="id"/>
        <result property="patientcode" column="patientcode"/>
        <result property="transmitting" column="transmitting"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectTemporaryqueueVo">
        select id, patientcode, transmitting
        from md_temporaryqueue
    </sql>

    <!-- 查询列表数据 -->
    <select id="getList" resultMap="TemporaryqueueMap">
        <include refid="selectTemporaryqueueVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="TemporaryqueueMap">
        <include refid="selectTemporaryqueueVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

