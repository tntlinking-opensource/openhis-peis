<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.dao.DictoccupationMapper">

    <resultMap type="com.center.medical.bean.model.Dictoccupation" id="DictoccupationMap">
        <id property="id" column="id"/>
        <result property="keyOccupation" column="key_occupation"/>
        <result property="occupationName" column="occupation_name"/>
        <result property="occupationCode" column="occupation_code"/>
        <result property="occupationCode2" column="occupation_code2"/>
        <result property="occupationCode3" column="occupation_code3"/>
        <result property="occupationCodehm" column="occupation_codehm"/>
        <result property="occupationcodex" column="occupationcodex"/>
        <result property="inputCode" column="input_code"/>
        <result property="note" column="note"/>
        <result property="status" column="status"/>
        <result property="createdate" column="createdate"/>
        <result property="modifydate" column="modifydate"/>
        <result property="isDelete" column="is_delete"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectDictoccupationVo">
        select id,
               key_occupation,
               occupation_name,
               occupation_code,
               occupation_code2,
               occupation_code3,
               occupation_codehm,
               occupationcodex,
               input_code,
               note,
               status,
               createdate,
               modifydate,
               is_delete
        from md_dictoccupation
    </sql>

    <!-- 查询列表数据 -->
    <select id="getList" resultMap="DictoccupationMap">
        <include refid="selectDictoccupationVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="DictoccupationMap">
        <include refid="selectDictoccupationVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

