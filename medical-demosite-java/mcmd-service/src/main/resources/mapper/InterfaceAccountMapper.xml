<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.dao.InterfaceAccountMapper">

    <resultMap type="com.center.medical.bean.model.InterfaceAccount" id="InterfaceAccountMap">
        <id property="id" column="id"/>
        <result property="createdate" column="createdate"/>
        <result property="modifydate" column="modifydate"/>
        <result property="username" column="username"/>
        <result property="password" column="password"/>
        <result property="key" column="key"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectInterfaceAccountVo">
        select id, createdate, modifydate, username, password, key
        from md_interface_account
    </sql>

    <!-- 查询列表数据 -->
    <select id="getList" resultMap="InterfaceAccountMap">
        <include refid="selectInterfaceAccountVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="InterfaceAccountMap">
        <include refid="selectInterfaceAccountVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>

