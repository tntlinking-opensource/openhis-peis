<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.medical.system.dao.DeployMdKsIpMapper">

    <resultMap type="com.center.medical.system.bean.model.MdKsIp" id="MdKsIpMap">
        <id property="id" column="id"/>
        <result property="name" column="name"/>
        <result property="ip" column="ip"/>
        <result property="ksId" column="ks_id"/>
        <result property="localPath" column="local_path"/>
        <result property="backupPath" column="backup_path"/>
        <result property="remark" column="remark"/>
        <result property="branchId" column="branch_id"/>
        <result property="status" column="status"/>
        <result property="createTime" column="create_time"/>
        <result property="updateTime" column="update_time"/>
        <result property="isEnableUpdate" column="is_enable_update"/>
        <result property="updateType" column="update_type"/>
    </resultMap>

    <!-- 数据表字段属性 -->
    <sql id="selectMdKsIpVo">
		select id, name, ip, ks_id, local_path, backup_path, remark, branch_id, status, create_time, update_time, is_enable_update, update_type		from md_ks_ip
	</sql>

    <!-- 查询列表数据 -->
    <select id="getPage" resultMap="MdKsIpMap">
        <include refid="selectMdKsIpVo"/>
    </select>

    <!-- 根据主键id获取记录详情 -->
    <select id="getInfoById" resultMap="MdKsIpMap">
        <include refid="selectMdKsIpVo"/>
        <where>
            id = #{id}
        </where>
    </select>

</mapper>
